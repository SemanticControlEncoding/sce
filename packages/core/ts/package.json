{
  "name": "@semanticencoding/core",
  "version": "0.0.2",
  "type": "commonjs",
  "engines": {
    "node": ">=24.0.0"
  },
  "description": "An open semantic layer enabling LLM-native communication protocols.  This is the core package providing foundational types, programatic utilities, and interfaces to use with Semantic Communication Encoding (SCE).  It's primary audience is to facilitate the development of SCE ecosystem tooling.  If you want to use SCE in your project, the semanticencoding package is a better fit.",
  "keywords": [
    "MCP",
    "LLM",
    "prompts",
    "semantic",
    "encoding",
    "SCE",
    "Semantic Encoding",
    "Semantic Communication Encoding"
  ],
  "author": {
    "name": "Sean O'Brien",
    "email": "seanmobrien@hotmail.com",
    "url": "https://github.com/seanmobrien/"
  },
  "bugs": {
    "url": "https://github.com/SemanticEncoding/sce/issues"
  },
  "homepage": "https://semanticencoding.github.io/sce/",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "EthicalUse-1.0",
  "files": [
    "dist/**/*",
    "LICENSE",
    "CHANGELOG.md",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepack": "node ../../../scripts/copy-shared-to-packages.js && tsc -b",
    "build": "tsc -b",
    "lint": "eslint --config ./eslint.config.cjs",
    "test": "jest --config ./jest.config.cjs",
    "test:cover": "jest --coverage --config ./jest.config.cjs"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SemanticEncoding/sce.git"
  }
}
